  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Renderer",
      "type": "chrome",
      "request": "attach",
      "port": 9223,
      "webRoot": "${workspaceFolder}/src",
      "timeout": 5000,
      "sourceMapPathOverrides": {
        "webpack:///./": "${webRoot}/",
        "webpack:///src/": "${webRoot}/",
        "webpack:///": ""
      }
    },
  ]
}
  
